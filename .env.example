# Configuration du bot Discord

# REQUIRED - Token du bot Discord
DISCORD_TOKEN=votre_token_ici

# REQUIRED - ID de l'application Discord
CLIENT_ID=votre_client_id_ici

# REQUIRED - ID du serveur Discord
GUILD_ID=votre_guild_id_ici

# OPTIONAL - ID du rôle à attribuer lors de l'acceptation du règlement (0 = désactivé)
VERIFIED_ROLE_ID=0

# OPTIONAL - ID du channel pour les logs Discord (0 = désactivé)
LOG_CHANNEL_ID=0

# OPTIONAL - IDs des rôles autorisés à créer des sondages, séparés par des virgules (0 = tout le monde)
# Exemple: POLL_ROLE_IDS=123456789,987654321
POLL_ROLE_IDS=0